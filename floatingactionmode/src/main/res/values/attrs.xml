<resources>
    <declare-styleable name="FloatingActionMode">
        <attr name="floatingActionModeBackgroundColor" format="color" />
        <attr name="floatingActionModeBackgroundCornerRadius" format="dimension" />
        <attr name="floatingActionModeElevation" format="dimension" />
        <attr name="floatingActionModeItemSize" format="dimension" />
        <attr name="floatingActionModeOverflowButtonBackground" format="reference|color" />
        <attr name="floatingActionModeOverflowButtonOverflowIcon" format="reference" />
        <attr name="floatingActionModeOverflowButtonBackIcon" format="reference" />
        <attr name="floatingActionModeOverflowButtonOverflowToBackIcon" format="reference" />
        <attr name="floatingActionModeOverflowButtonBackToOverflowIcon" format="reference" />
        <attr name="floatingActionModeOverflowButtonIconTint" format="color" />
        <attr name="floatingActionModeOverflowButtonIconTintMode" format="enum">
            <enum name="src_over" value="3" />
            <enum name="src_in" value="5" />
            <enum name="src_atop" value="9" />
            <enum name="multiply" value="14" />
            <enum name="screen" value="15" />
            <enum name="add" value="16" />
        </attr>
        <attr name="floatingActionModeOverflowButtonOverflowContentDescription" format="string" />
        <attr name="floatingActionModeOverflowButtonBackContentDescription" format="string" />
        <attr name="floatingActionModeMargin" format="dimension" />
        <attr name="floatingActionModeItemBackground" format="reference|color" />
        <attr name="floatingActionModeItemPadding" format="dimension" />
        <attr name="floatingActionModeItemPaddingEdge" format="dimension" />
        <attr name="floatingActionModeItemPaddingIcon" format="dimension" />
        <attr name="floatingActionModeItemIconWidth" format="dimension" />
        <attr name="floatingActionModeItemTextPaddingIcon" format="dimension" />
        <attr name="floatingActionModeItemTextAppearance" format="reference" />
        <attr name="floatingActionModeItemTextColor" format="color" />
        <attr name="floatingActionModeItemTextSize" format="dimension" />
        <attr name="floatingActionModeOverflowItemMinimumWidth" format="dimension" />
        <attr name="floatingActionModeOverflowItemMaxShowCount" format="float" />
        <attr name="floatingActionModeSubItemMinimumWidth" format="dimension" />
        <attr name="floatingActionModeSubItemMaxShowCount" format="float" />
        <attr name="floatingActionModeSubTitleTextAppearance" format="reference" />
        <attr name="floatingActionModeSubTitleTextColor" format="color" />
        <attr name="floatingActionModeSubTitleTextSize" format="dimension" />
        <attr name="floatingActionModeAnimationInterpolator" format="reference" />
        <attr name="floatingActionModeAnimationDuration" format="integer" />
        <attr name="floatingActionModeAnimationDurationAdjustmentUnit" format="integer" />
        <attr name="floatingActionModeWindowAnimations" format="reference" />
    </declare-styleable>
</resources>